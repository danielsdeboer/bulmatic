{
  "name": "bulmatic",
  "version": "0.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/danielsdeboer/bulmatic"
  },
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js",
    "test-watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack --watch --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js"
  },
  "description": "Bulma components for Vue",
  "dependencies": {
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "eslint-config-dbt": "^0.1.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "expect": "^21.2.1",
    "file-loader": "^1.1.4",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "lodash.pick": "^4.4.0",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "node-sass": "^4.9.4",
    "nyc": "^11.9.0",
    "path": "^0.12.7",
    "sass-loader": "^7.1.0",
    "vue-loader": "^13.7.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
